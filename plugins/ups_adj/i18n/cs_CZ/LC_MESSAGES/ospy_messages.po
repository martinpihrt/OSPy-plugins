#: plugins/ups_adj/__init__.py:34
msgid "Report from OSPy UPS plugin"
msgstr "Zpráva z rozšíření UPS"

#: plugins/ups_adj/__init__.py:103 plugins/ups_adj/__init__.py:364
msgid "OK"
msgstr "V pořádku"

#: plugins/ups_adj/__init__.py:105 plugins/ups_adj/__init__.py:366
msgid "FAULT"
msgstr "Chyba"

#: plugins/ups_adj/__init__.py:115 plugins/ups_adj/__init__.py:116
msgid "Detected fault on power line."
msgstr "Zjištěna závada na elektrickém vedení."

#: plugins/ups_adj/__init__.py:115 plugins/ups_adj/__init__.py:116
#: plugins/ups_adj/__init__.py:140 plugins/ups_adj/__init__.py:141
#: plugins/ups_adj/__init__.py:154 plugins/ups_adj/__init__.py:155
