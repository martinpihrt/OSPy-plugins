msgstr "Stáhnout soubor se záznamem jako"

#: plugins/ups_adj/templates/ups_adj.html:175
msgid "If logging is enabled, a graph with measured data will be displayed at the bottom of the screen"
msgstr "Je-li povolen záznam, zobrazí se v dolní části obrazovky graf s naměřenými daty"

#: plugins/ups_adj/templates/ups_adj.html:209
msgid "Enable logging"
msgstr "Povolit záznam a graf"

#: plugins/ups_adj/templates/ups_adj.html:227
msgid "Delete all logs"
msgstr "Smazat všechny logy"

#: plugins/ups_adj/templates/ups_adj.html:236
msgid "Clear selection"
msgstr "Zrušit výběr"

#: plugins/ups_adj/templates/ups_adj.html:236
msgid "Measured status on/off"
msgstr "Měřené jako zap/vyp"

#: plugins/ups_adj/templates/ups_adj.html:236
msgid "Reason"
msgstr "Statistika"

#: plugins/ups_adj/templates/ups_adj.html:236
msgid "on/off"
msgstr "zap/vyp"

